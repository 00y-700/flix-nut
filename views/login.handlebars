<div class="col-lg-12">
    <div class="card">
        <div class="card-body">
            <form action="/login" method="POST">
                <p>Email</p>
                <input type="email" id="email" name="email" required>
                <p>Password</p>
                <input type="password" id="password" name="password" required>
                <button type="submit" id="login">Log In</button>
                <p>Not A Member?</p>
                <a href="/register">Register</a>
            </form>
        </div>
    </div>
</div>
